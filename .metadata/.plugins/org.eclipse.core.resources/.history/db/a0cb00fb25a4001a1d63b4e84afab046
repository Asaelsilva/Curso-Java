package Colecoes;

import java.util.HashSet;

public class ConjuntoBaguncado {

	public static void main(String[] args) {

		HashSet conjunto = new HashSet();
		
		//Adicionando itens ao conjunto
		conjunto.add(1.2); //double -> Double
		conjunto.add(true);
		conjunto.add("Teste");
		conjunto.add(1);
		conjunto.add("A");
		
		
		System.out.println("O tamanho é " + conjunto.size());
		
		conjunto.add("Teste");
		System.out.println("O tamanho é " + conjunto.size());
		
		//removendo
		System.out.println(conjunto.remove(1));
		System.out.println(conjunto.remove(2));
		System.out.println("O tamanho é " + conjunto.size());
		
		// verificando item: "A" esta conteda no conjunto? True
		System.out.println(conjunto.contains("A"));
		
		
		
 	}

}
