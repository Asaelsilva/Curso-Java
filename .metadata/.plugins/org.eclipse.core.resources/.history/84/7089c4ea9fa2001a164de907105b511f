package classe;

public class Pessoa {

	String nome;
	double peso;

	public Pessoa(String nome, double peso) {
		
		this.nome = nome;
		this.peso = peso;
	}

	public double Comer(double peso) {

		var pesoFinal = this.peso + peso;

		return pesoFinal;
	}

	public double Comeu(double peso) {

		var pesoDepois = Comer(peso)

		return pesoFinal;
	}

}
