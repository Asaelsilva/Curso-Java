package oo.composicao;

import java.util.ArrayList;
import java.util.List;

public class Compra {

	String cliente;
	
	List<Item> items = new ArrayList<>();
	
	double obterValorTotal(){
		 
		double total = 0;
		
		for (Item item : items) {
			total = total + (item.qtd * item.preco) 
		}
		
		
		return 0;
	 }
}
