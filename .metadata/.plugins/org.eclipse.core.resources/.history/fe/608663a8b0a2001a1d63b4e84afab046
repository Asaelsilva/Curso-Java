package arrays;

import java.util.Arrays;

public class Exercicio1 {

	
	public static void main(String[] args) {
		
		double[] notasAlunoA = new double[3];
		
		notasAlunoA[0] = 8.0;
		notasAlunoA[1] = 5.5;
		notasAlunoA[2] = 7.2;
		
				
		System.out.println(Arrays.toString(notasAlunoA));
		
		
		for (int i = 0; i < 3; i++) {
			
			System.out.println(notasAlunoA[i]);
			
		}
	}
}
