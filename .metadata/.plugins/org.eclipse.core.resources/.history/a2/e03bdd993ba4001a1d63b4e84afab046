package Colecoes;

import java.util.LinkedList;
import java.util.Queue;

public class Fila {

	public static void main(String[] args) {
		
		Queue<String> fila = new LinkedList<String>();
		
		//Offer e Add -> Adicionam elementos na fila
		// diferença é op comportamento quando a fila esta cheia
		fila.add("Ana");
		fila.offer("Bia");
		fila.offer("Marco");
		fila.offer("Lorrayne");
		fila.offer("Paulo");
		fila.offer("Luis");
		fila.offer("Bulma");

	}

}
