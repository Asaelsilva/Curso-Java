package Colecoes;

import java.util.HashMap;
import java.util.Map;

public class Mapa {

	public static void main(String[] args) {

		Map<Integer, String> usuario = new HashMap<>();
	
		usuario.put(1, "Roberto");
		usuario.put(2, "Asael");
		usuario.put(3, "Bia");
		usuario.put(4, "Rafaela");
		usuario.put(5, "Paula");
		
		
		System.out.println(usuario.size());
	
	
	
	}

}
